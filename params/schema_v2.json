{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "FDBParameterSetV2",
  "type": "object",
  "required": ["theta_opt", "theta_if", "theta_aniso", "theta_cos"],
  "properties": {
    "theta_opt": {
      "type": "object",
      "required": ["tau0", "omega0", "p"],
      "properties": {
        "tau0": {"type": "number", "minimum": 0.0},
        "omega0": {"type": "number", "minimum": 0.0, "maximum": 1.0},
        "p": {"type": "number", "minimum": 0.5, "maximum": 2.5}
      }
    },
    "theta_if": {
      "type": "object",
      "required": ["eta", "s_gate", "q_knee"],
      "properties": {
        "eta": {"type": "number", "minimum": 0.0},
        "s_gate": {"type": "number", "minimum": 0.0},
        "q_knee": {"type": "number", "minimum": 0.0, "maximum": 1.0}
      }
    },
    "theta_aniso": {
      "type": "object",
      "required": ["g"],
      "properties": {
        "g": {"type": "number", "minimum": 0.0, "maximum": 1.0}
      }
    },
    "theta_cos": {
      "type": "object",
      "required": ["epsilon", "s", "k0", "q", "m", "k_c", "n"],
      "properties": {
        "epsilon": {"type": "number"},
        "s": {"type": "number"},
        "k0": {"type": "number"},
        "q": {"type": "number"},
        "m": {"type": "number"},
        "k_c": {"type": "number"},
        "n": {"type": "number", "minimum": 0.0}
      }
    }
  },
  "additionalProperties": false
}
